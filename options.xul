	var pls = Components.classes['@mozilla.org/pref-localizedstring;1']
                    .createInstance(Components.interfaces.nsIPrefLocalizedString);
	pls.data = this.value;
	Components.classes['@mozilla.org/preferences-service;1'].getService(Components.interfaces.nsIPrefService)
		.setComplexValue('extensions.send-to-dune@jetpack.ytresolution',  Components.interfaces.nsIPrefLocalizedString, pls);
		var pls = Components.classes['@mozilla.org/pref-localizedstring;1']
                    .createInstance(Components.interfaces.nsIPrefLocalizedString);
	pls.data = this.value;
	Components.classes['@mozilla.org/preferences-service;1'].getService(Components.interfaces.nsIPrefService)
		.setComplexValue('extensions.send-to-dune@jetpack.ytresolution',  Components.interfaces.nsIPrefLocalizedString, pls);
			">
   <radio id="a1" label="360p (default)" tooltiptext="360p (default)" value="360p"></radio>
   <radio id="a2" label="720p HD" tooltiptext="720p HD" value="720p"></radio>
   <radio id="a3" label="1080p HD" tooltiptext="1080p HD" value="1080p"></radio>
  </radiogroup>
