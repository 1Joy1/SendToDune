   </setting>
   
     <setting data-jetpack-id="send-to-dune@jetpack" pref="extensions.send-to-dune@jetpack.ytresolution" pref-name="ytresolution" title="Video from YOUTUBE, will be playback with the quality of"  disabled="true" type="string">
